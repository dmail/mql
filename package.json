{
  "name": "mql",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "node ./scripts/dev.mjs",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "build": "node ./scripts/build.mjs",
    "build:serve": "node ./scripts/build_serve.mjs --open",
    "eslint": "npx eslint . --ext=.html,.js,.jsx,.mjs,.cjs",
    "prettier": "prettier --write .",
    "playwright:install": "npx playwright install-deps && npx playwright install"
  },
  "workspaces": [
    "./packages/*"
  ],
  "dependencies": {
    "preact": "10.25.0",
    "@preact/signals": "1.3.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "7.26.0",
    "@babel/plugin-transform-react-jsx": "7.25.9",
    "@jsenv/custom-elements-redefine": "0.0.1",
    "@jsenv/assert": "4.4.2",
    "@jsenv/core": "39.6.0",
    "@jsenv/eslint-config-relax": "1.3.4",
    "@jsenv/plugin-bundling": "2.7.19",
    "@jsenv/plugin-preact": "1.6.28",
    "@jsenv/test": "3.5.28",
    "@playwright/browser-chromium": "1.49.0",
    "prettier-plugin-organize-imports": "4.1.0",
    "eslint": "9.15.0",
    "open": "10.1.0",
    "prettier": "3.4.0"
  }
}
